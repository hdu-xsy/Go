<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">
<filter>
   <filter-name>filter</filter-name>
   <filter-class>filter</filter-class>
   <init-param>
      <param-name>test-param</param-name>
      <param-value>Initialization Paramter</param-value>
   </init-param>
</filter>

<filter-mapping>
   <filter-name>filter</filter-name>
   <url-pattern>/*</url-pattern>
</filter-mapping>


<servlet>
    <servlet-name>paper_login_confirm</servlet-name>
    <servlet-class>paper_login_confirm</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>paper_login_confirm</servlet-name>
    <url-pattern>/paper_login_confirm</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>paper_delete_confirm</servlet-name>
    <servlet-class>paper_delete_confirm</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>paper_delete_confirm</servlet-name>
    <url-pattern>/paper_delete_confirm</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>paper_motify_confirm</servlet-name>
    <servlet-class>paper_motify_confirm</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>paper_motify_confirm</servlet-name>
    <url-pattern>/paper_motify_confirm</url-pattern>
</servlet-mapping>


<listener>
        <listener-class>
            onlineListener
        </listener-class>
</listener>


<session-config>
        <session-timeout>1</session-timeout>
</session-config>

</web-app>
